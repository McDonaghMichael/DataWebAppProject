<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lecturers</title>
</head>
<body>
    <h1>Lecturers List</h1>
    <table>
        <thead>
            <tr>
                <th>Lecturer ID</th>
                <th>Name</th>
                <th>Department</th>
            </tr>
        </thead>
        <tbody>
            <% lecturers.forEach(lecturer => { %>
                <tr>
                    <td><%= lecturer._id %></td>
                    <td><%= lecturer.name %></td>
                    <td><%= lecturer.did %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
